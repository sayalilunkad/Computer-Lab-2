#include<stdio.h>
#include<stdlib.h>
#include<pthread.h>

pthread_t id[3];
int clock_t_1=0, clock_t_2=0, clock_t_3=0;

main()
{
	int interval[3];
	interval={5,10,15};
	
	pthread_create(&id[0], NULL, &clock1, (void *)&interval[0]);
	pthread_create(&id[1], NULL, &clock2, (void *)&interval[1]);
	pthread_create(&id[2], NULL, &clock3, (void *)&interval[2]);

}

